{
  "name": "health-check-server",
  "main": "dist/server.js",
  "scripts": {
    "prepare:sdk": "cd ../ && yarn build",
    "build": "yarn prepare:sdk && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "nodemon --watch '../src' --ext ts --exec ts-node -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "@types/express": "^5.0.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.0"
  }
}
