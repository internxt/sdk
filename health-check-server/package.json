{
  "name": "health-check-server",
  "main": "dist/server.js",
  "scripts": {
    "prepare:sdk": "cd ../ && yarn build",
    "build": "yarn prepare:sdk && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "nodemon --watch './' --ext ts --exec ts-node -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "@types/express": "^5.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "openapi-fetch": "^0.14.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "nodemon": "^3.1.10",
    "openapi-typescript": "^7.8.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.0"
  }
}
